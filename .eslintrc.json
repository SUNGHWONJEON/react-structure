{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "rules": {
    // 여기에 사용할 규칙을 추가하세요.
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn"
    ]
  }
}